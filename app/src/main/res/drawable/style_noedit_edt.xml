<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="3dp" android:color="#ff000d"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item
        android:left="5dp"
        android:right="5dp"
        android:bottom="5dp">
        <shape android:shape="rectangle">
            <solid android:color="#00466e"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
</layer-list>